{
  "expo": {
    "name": "Reslan Alnaal Center Employee App",
    "slug": "reslan_alnaal_center",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app-icon.png",
    "scheme": "reslanalnaalcenter",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "ExpoLocalization_supportsRTL": true,
        "ExpoLocalization_forcesRTL": true,
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "reslan.alnaal.center"
    },
    "android": {
      "package": "reslan.alnaal.center",
      "icon": "./assets/images/app-icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-localization",
        {
          "supportedLocales": {
            "ios": ["en", "ja"],
            "android": ["en", "ja"]
          }
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash.png",
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-notifications",
      "@react-native-firebase/app",
      "@react-native-firebase/messaging",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Kodchasan/Kodchasan-Regular.ttf",
            "./assets/fonts/Cairo/Cairo-Regular.ttf"
          ],
          "android": {
            "fonts": [
              {
                "fontFamily": "kodchasan",
                "fontDefinitions": [
                  {
                    "path": "./assets/fonts/Kodchasan/Kodchasan-Bold.ttf",
                    "weight": 700
                  },
                  {
                    "path": "./assets/fonts/Kodchasan/Kodchasan-Light.ttf",
                    "weight": 300
                  },
                  {
                    "path": "./assets/fonts/Kodchasan/Kodchasan-ExtraLight.ttf",
                    "weight": 200
                  },
                  {
                    "path": "./assets/fonts/Kodchasan/Kodchasan-Medium.ttf",
                    "weight": 500
                  },
                  {
                    "path": "./assets/fonts/Kodchasan/Kodchasan-SemiBold.ttf",
                    "weight": 600
                  }
                ]
              },
              {
                "fontFamily": "cairo",
                "fontDefinitions": [
                  {
                    "path": "./assets/fonts/Cairo/Cairo-Bold.ttf",
                    "weight": 700
                  },
                  {
                    "path": "./assets/fonts/Cairo/Cairo-Light.ttf",
                    "weight": 300
                  },
                  {
                    "path": "./assets/fonts/Cairo/Cairo-ExtraLight.ttf",
                    "weight": 200
                  },
                  {
                    "path": "./assets/fonts/Cairo/Cairo-Medium.ttf",
                    "weight": 500
                  },
                  {
                    "path": "./assets/fonts/Cairo/Cairo-SemiBold.ttf",
                    "weight": 600
                  },
                  {
                    "path": "./assets/fonts/Cairo/Cairo-ExtraBold.ttf",
                    "weight": 800
                  },
                  {
                    "path": "./assets/fonts/Cairo/Cairo-Black.ttf",
                    "weight": 400
                  }
                ]
              }
            ]
          }
        }
      ],
      [
        "expo-calendar",
        {
          "calendarPermission": "The app needs to access your calendar."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "2d3b00aa-7131-4841-8ec1-b1e2e704f3ee"
      },
      "supportsRTL": true
    },
    "owner": "khaldounalhalabi"
  }
}
